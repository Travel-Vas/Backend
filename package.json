{
  "name": "Photolocker Api",
  "version": "1.0.0",
  "description": "Photolocker Backend Api",
  "main": "app.js",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node ./build/src/app.js\"",
    "start": "node ./build/src/app.js",
    "start:prod": "pm2 startOrReload pm2config.json",
    "start-cpanel": "node ./dist/app.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "tsc",
    "build-dist": "tsc --outDir ./dist && npm run copy-dist",
    "copy-package": "copyfiles -f ./package.json ./build",
    "copy-dist": "copyfiles -f ./package.json ./dist",
    "test": "jest --verbose",
    "test:stage": "npm run test -- --findRelatedTests",
    "test:ci": "npm run test -- --coverage"
  },
  "keywords": [
    "photolocker",
    "virtual studio"
  ],
  "author": "(Abraham Jude)",
  "license": "ISC",
